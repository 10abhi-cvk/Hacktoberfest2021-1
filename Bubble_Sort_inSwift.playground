//Bubble Sort algorithm for Swift
//Enter any array in the line 6

import UIKit

var arrNumbers:[Int] = [64, 34, 25, 12, 22, 11, 90]
var n = arrNumbers.count
var swap = true
while swap == true {
    swap = false
    for i in 0 ..< n-1 {
        if arrNumbers[i] > arrNumbers[i + 1] {
            let temp = arrNumbers [i + 1]
            arrNumbers [i + 1] = arrNumbers[i]
            arrNumbers[i] = temp
            swap = true
        }
    }
}

print (arrNumbers)

